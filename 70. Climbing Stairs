class Solution {
public:
int solve(int n){
    if(n<=2){
        return n;
    }
     int prev1= 1;
     int prev2= 2;
     int curr;
     for(int i=3; i<=n; i++){
         curr= prev2+prev1;
         prev1 =prev2;
         prev2= curr;
     }
     return curr;
}
    int climbStairs(int n) {
        return solve(n);
    }
};
